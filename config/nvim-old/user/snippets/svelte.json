{
  "setup": {
    "prefix": ["setup", "template"],
    "body": [
      "<script>",
      "\t$1",
      "</script>",
      "",
      "$2",
      "",
      "<style>",
      "\t$3",
      "</style>"
    ],
    "description": "Standard setup for a Svelte file"
  },
  "setup:ts": {
    "prefix": ["setup:ts", "template:ts"],
    "body": [
      "<script lang=\"ts\">",
      "\t$1",
      "</script>",
      "",
      "$2",
      "",
      "<style>",
      "\t$3",
      "</style>"
    ],
    "description": "Standard setup for a TS Svelte file"
  }
}
